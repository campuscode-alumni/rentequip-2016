<div class="page-header">
   <h1>Lista de Clientes</h1>
</div>
<div class="row">
  <div class="col-md-12 col-sm-12">
      <table id="clients" class='table table-striped'>
        <thead>
          <tr>
            <th>Nome Fantasia</th>
            <th>CNPJ</th>
            <th>Estado</th>
          </tr>
        </thead>
        <tbody>
          <% @clients.each do |client| %>
          <tr>
            <td><%= link_to client.fantasy_name, client %></td>
            <td><%= client.cnpj %></td>
            <td><%= client.state %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
      <%= link_to 'Novo', new_client_path,class: "btn btn-success pull-right" %>
  </div>
</div>
